            <h6 class="white shooting-name"> "Sauvage"</h6>
            <p class="white shooting-text"> Crédit photo: Ghjuliu Mattei </p>
          </div>
          <div class="back">
            <div class="pic-cont" data-performances-target="picsconts">
              <%= image_tag "Sauvage-1.jpg", class: "performance-pic seen-one" %>
              <%= image_tag "Sauvage-3.jpg", class: "performance-pic visibility", data: { performances_target: "picturesgoright" } %>
              <%= image_tag "Sauvage-4.jpg", class: "performance-pic visibility", data: { performances_target: "picturesgomoreright" } %>
              <%= image_tag "Sauvage 2.jpg", class: "performance-pic visibility", data: { performances_target: "picturesgodown" } %>
            </div>
          </div>
        </div>
        <button class= "scaled-button front-but" data-action="click->performances#show_shooting_one" data-performances-target="buttons"> ouvrir </button>
